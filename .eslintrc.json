{
    "env": {
        "browser": true,
        "commonjs": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "new-cap": "warn",
        "max-len": "warn",
      "object-curly-spacing": "warn",
        "key-spacing": "warn",
      "valid-jsdoc": "warn",
      "no-unused-vars": "warn",
      "no-redeclare": "warn",
      "comma-spacing": "warn"
    }
}
